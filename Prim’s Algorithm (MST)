#include <stdio.h>
#define INF 9999
#define MAX 10

void prims(int graph[MAX][MAX], int n) {
    int selected[MAX] = {0}, no_of_edges = 0;
    selected[0] = 1;

    printf("Edges in Minimum Spanning Tree:\n");
    while (no_of_edges < n - 1) {
        int min = INF, x = 0, y = 0;
        for (int i = 0; i < n; i++) {
            if (selected[i]) {
                for (int j = 0; j < n; j++) {
                    if (!selected[j] && graph[i][j]) {
                        if (min > graph[i][j]) {
                            min = graph[i][j];
                            x = i;
                            y = j;
                        }
                    }
                }
            }
        }
        printf("%d - %d : %d\n", x, y, graph[x][y]);
        selected[y] = 1;
        no_of_edges++;
    }
}

int main() {
    int n, graph[MAX][MAX];
    printf("Enter number of vertices: ");
    scanf("%d", &n);
    printf("Enter adjacency matrix (0 for no edge):\n");
    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++)
            scanf("%d", &graph[i][j]);

    prims(graph, n);
    return 0;
}
