#include <stdio.h>

int main() {
    int n, i;
    long a = 0, b = 1, sum = 1, next;
    printf("Enter number of terms: ");
    scanf("%d", &n);

    if (n == 1) sum = 0;
    for (i = 2; i < n; i++) {
        next = a + b;
        sum += next;
        a = b;
        b = next;
    }
    printf("Sum of Fibonacci series: %ld\n", sum);
    return 0;
}
