#include <stdio.h>
#define SIZE 10

int hashTable[SIZE];

void initTable() {
    for (int i = 0; i < SIZE; i++)
        hashTable[i] = -1;  // -1 means empty slot
}

int hashFunction(int key) {
    return key % SIZE;
}

void insert(int key) {
    int index = hashFunction(key);
    int i = 0;
    while (hashTable[(index + i) % SIZE] != -1) {
        i++;
        if (i == SIZE) {
            printf("Hash Table is full\n");
            return;
        }
    }
    hashTable[(index + i) % SIZE] = key;
}

void display() {
    printf("Hash Table:\n");
    for (int i = 0; i < SIZE; i++)
        printf("%d --> %d\n", i, hashTable[i]);
}

int main() {
    initTable();
    insert(5);
    insert(15);
    insert(25);
    insert(35);
    insert(7);
    display();
    return 0;
}
