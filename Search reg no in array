#include <stdio.h>

int main() {
    int arr[50], n, i, key, found = 0;
    printf("Enter number of reg nos: ");
    scanf("%d", &n);
    printf("Enter reg nos: ");
    for (i = 0; i < n; i++) scanf("%d", &arr[i]);

    printf("Enter reg no to search: ");
    scanf("%d", &key);

    for (i = 0; i < n; i++) {
        if (arr[i] == key) {
            found = 1;
            break;
        }
    }
    if (found) printf("Reg no found at position %d\n", i + 1);
    else printf("Not found\n");
    return 0;
}
